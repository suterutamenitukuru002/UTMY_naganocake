<div class="container">
  <div class='mx-auto col-md-5'>
    <h1>ジャンル編集</h1>
      <tr>
        <%= form_with model: [:admin, @genre], local:true do |f| %>
          <div class="form-group">
            <%= f.label :ジャンル名 %>
            <%= f.text_field :name, class: 'form-control' %>
          </div>
            <div class="form-group">
              <%= f.submit "変更を保存", class: 'btn btn-info' %>
            </div>
              <% end %>
  </div>
  <%= image_tag 'Cutlery.png',size:'10,30' %>
</div>