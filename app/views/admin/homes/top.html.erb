<h1>注文履歴一覧</h1>
　<table class="table">

 <thead>
  <tr>
  <th>購入日時</th>
  <th>購入者</th>
  <th>注文個数</th>
  <th>注文ステータス</th>
  </tr>
 </thead>

<tbody>
 <tr><% @orders.each do |order| %></tr>
 <td><%= orders.created_at.strftime('%Y/%m/%d %H:%M') %></td>
 <td><%= @customer.full_name %></td>
 <td><%= @order_derails.quantity %></td>
 <td><%= order_status %></td>
 <% end %>
</tbody>

</table>